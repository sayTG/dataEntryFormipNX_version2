<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dataentryformipnx_2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permision.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>

    <application
        tools:ignore="LockedOrientationActivity"
        android:allowBackup="true"
        android:icon="@mipmap/ipnx_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ipnx_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.ViewCritical"
            android:label="Critical Data"
            android:screenOrientation="portrait"/>
        <activity android:name=".view.ViewRemediation"
            android:label="Remediation Data"
            android:screenOrientation="portrait"/>
        <activity android:name=".view.ViewCorrective"
            android:label="Corrective Data"
            android:screenOrientation="portrait"/>
        <activity android:name=".view.ViewPreventive"
            android:label="Preventive Data"
            android:screenOrientation="portrait"/>
        <activity android:name=".ui.data_entry.PreventiveMaintenance"
            android:label="Preventive Sheet"
            android:screenOrientation="portrait"/>
        <activity android:name=".ui.data_entry.CorrectiveMaintenance"
            android:label="Corrective Sheet"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".login_package.Choose"
            android:label="Choose"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".login_package.Log_in_class"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
    </application>

</manifest>